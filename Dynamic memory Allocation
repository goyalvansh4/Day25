#include<iostream>
using namespace std;

int main(){

int *ptr = new int;
 *ptr=5;
 
 cout<<*ptr<<endl;
 
 int n;
 cout<<"Enter the value of n"<<endl;
 cin>>n;
 int *arr=new int[n];
 
 for(int i=0;i<n;i++){
   cin>>arr[i];
 }
 for(int i=0;i<n;i++){
   cout<<arr[i]<<" ";
 }cout<<endl;
 int row,col;
 cout<<"Enter the row and col"<<endl;
 cin>>row>>col;
 int **arr2=new int*[row];
 for(int i=0;i<row;i++){
   arr2[i]=new int[col];
 }
 
 for(int i=0;i<row;i++){
   for(int j=0;j<col;j++){
   cin>>arr2[i][j];
  }
} 

for(int i=0;i<row;i++){
   for(int j=0;j<col;j++){
   cout<<arr2[i][j]<<" ";
  }cout<<endl;
} 
 
 delete ptr;
 
 delete []arr;
 
for(int i=0;i<row;i++){
   delete []arr2[i];
 }
 
 delete []arr2;
 }
 
